{
  "name": "obd-parser",
  "version": "0.1.3",
  "description": "OBD interface that can send and receive messages over a given connection.",
  "main": "lib/obd-interface.js",
  "dependencies": {
    "@types/bluebird": "~3.0.35",
    "@types/bunyan": "0.0.32",
    "@types/node": "~6.0.46",
    "@types/pino": "~1.0.4",
    "@types/ramda": "0.0.2",
    "@types/verror": "~1.6.28",
    "bluebird": "~3.2.2",
    "bunyan": "~1.8.4",
    "case": "~1.4.2",
    "chai": "~3.5.0",
    "pino": "~2.15.0",
    "ramda": "~0.22.1",
    "verror": "~1.6.1"
  },
  "devDependencies": {
    "chai-as-promised": "~5.2.0",
    "istanbul": "~0.4.2",
    "jshint": "~2.9.1",
    "linelint": "~1.0.0",
    "mocha": "~2.4.5",
    "proxyquire": "~1.7.4",
    "sinon": "~1.17.3",
    "typescript": "~2.0.10"
  },
  "scripts": {
    "compile": "tsc lib/obd-interface.ts",
    "unit": "env NODE_PATH=. mocha $(find . -name \"*.test.js\" -not -path \"*/node_modules/*\")",
    "lint": "npm run-script jshint && npm run-script linelint",
    "jshint": "jshint $(find . -name \"*.js\" -not -path \"*/test/*\" -not -path \"*/coverage/*\" -not -path \"*/node_modules/*\")",
    "linelint": "linelint $(find . -name \"*.js\" -not -path \"*/test/*\" -not -path \"*/coverage/*\" -not -path \"*/node_modules/*\")",
    "check-coverage": "istanbul check-coverage --statements 90 --branches 90 --functions 90 --lines 90",
    "test": "npm run-script lint && env NODE_PATH=. istanbul cover -x *.test.js _mocha $(find . -name \"*.test.js\" -not -path \"*/node_modules/*\") && npm run-script check-coverage"
  },
  "keywords": [
    "obd",
    "parser",
    "message",
    "obdii",
    "obd2"
  ],
  "author": "Evan Shortiss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/evanshortiss/obd-parser.git"
  },
  "bugs": {
    "url": "https://github.com/evanshortiss/obd-parser/issues"
  },
  "homepage": "https://github.com/evanshortiss/obd-parser"
}
